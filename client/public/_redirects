# Cloudflare Pages Redirects Configuration
# Format: from to [status]

# API routes are handled by the Worker function
/api/* /api/:splat 200

# WebSocket routes handled by Worker
/ws/* /ws/:splat 200

# Note: SPA fallback is handled by wrangler.jsonc "not_found_handling": "single-page-application"
# Do NOT add /* /index.html 200 here - it causes infinite loop errors
